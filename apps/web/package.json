{
  "name": "@dflow/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS='--no-deprecation --trace-warnings' NEXT_DEBUG=1 next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "type-check": "tsc --noEmit",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload"
  },
  "dependencies": {
    "@dflow/actions": "workspace:*",
    "@dflow/components": "workspace:*",
    "@dflow/hooks": "workspace:*",
    "@dflow/lib": "workspace:*",
    "@dflow/payload": "workspace:*",
    "@dflow/providers": "workspace:*",
    "@dflow/stores": "workspace:*",
    "@dflow/types": "workspace:*",
    "@bprogress/next": "^3.2.10",
    "@content-collections/core": "^0.8.2",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/next": "^0.2.7",
    "@logtail/next": "^0.2.0",
    "@payloadcms/db-mongodb": "3.54.0",
    "@payloadcms/email-resend": "3.54.0",
    "@payloadcms/next": "3.54.0",
    "@payloadcms/plugin-multi-tenant": "3.54.0",
    "@payloadcms/richtext-lexical": "3.54.0",
    "@payloadcms/sdk": "^3.59.1",
    "@payloadcms/ui": "3.54.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.1",
    "payload": "3.54.0",
    "posthog-js": "^1.239.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "5.7.3"
  }
}
